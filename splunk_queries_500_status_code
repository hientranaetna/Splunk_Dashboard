mycolumn=value
| STATS avg(mycolumn) BY mycolumn
| FIELDS mycolumn, avg(mycolumn)

status=“500 Internal Server Error”
| STATS status BY time
| FIELDS time, status


index="cloudeng_aks" cluster_name="aks-corp-util-dev-usc" container_name="enterprise-technology-services-api-dev" status="500 Internal Server Error" | STATS count(status) BY _time

index="cloudeng_aks" cluster_name="aks-corp-util-dev-usc" container_name="enterprise-technology-services-api-dev" eventtype=err0r status="500 Internal Server Error" | bucket span=10m _time | STATS count(status) BY _time
